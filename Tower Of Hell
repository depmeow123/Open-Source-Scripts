local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/wizard"))()
 
local PhantomForcesWindow = Library:NewWindow("dep#6969")

local KillingCheats = PhantomForcesWindow:NewSection("Main")

KillingCheats:CreateButton("Teleport To Finish", function()
    for i,v in pairs(game:GetService("Workspace").tower.sections.finish:GetChildren()) do
        if v.Name == "FinishGlow" then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
        end
    end
    end)
    
    KillingCheats:CreateButton("Godmode", function()
        for i,v in pairs(workspace.tower:GetDescendants()) do
            if v.Name == "kills" then
                print(v.Parent)
                v:Destroy()
            end
        end
        end)
    
    KillingCheats:CreateButton("Delete Kill Parts", function()
        for k,v in pairs(game:GetService("Workspace").tower:GetDescendants()) do
            if string.match(v.Name,"kill") then
                print(v)
                v:Destroy()
                end
            end
        end)
   
    KillingCheats:CreateButton("Infinite Jump", function()
        game:GetService("ReplicatedStorage").globalJumps.Value = 6969
        end)

    KillingCheats:CreateSlider("Walkspeed", 16, 50, 16, false, function(value)
        game:GetService("ReplicatedStorage").globalSpeed.Value = value
     end)

     KillingCheats:CreateSlider("Jump Power", 50, 200, 50, false, function(value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
         end)
